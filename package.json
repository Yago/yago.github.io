    "gatsby-source-filesystem": "^2.1.40",
    "gatsby-transformer-remark": "^2.6.39",
    "gatsby-transformer-sharp": "^2.3.7",
    "graphql-skip-limit": "^2.1.21",
    "highcharts": "^7.2.1",
    "highcharts-react-official": "^2.2.2",
    "javascript-terminal": "^1.0.3",
