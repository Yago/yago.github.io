{
  "name": "with-typescript-types",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf .next && rm -rf node_modules/.cache",
    "tailwind:build": "yarn run clean && tailwind build src/styles/tailwind.css -o src/styles/base.css -c ./tailwind.config.js",
    "dev": "yarn run tailwind:build && next",
    "build": "yarn run tailwind:build && next build",
    "start": "next start",
    "type-check": "tsc",
    "lint:js": "eslint --ext js,jsx,ts,tsx ./ --max-warnings=0",
    "postinstall": "husky install && patch-package",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "dependencies": {
    "@emotion/babel-preset-css-prop": "^11.2.0",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "@mdx-js/loader": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@next/mdx": "^10.0.7",
    "@tailwindcss/aspect-ratio": "^0.2.0",
    "@tailwindcss/typography": "^0.4.0",
    "autoprefixer": "^10.2.4",
    "directory-tree": "^2.2.7",
    "framer-motion": "^3.7.0",
    "javascript-terminal": "^1.1.1",
    "next": "10.0.7",
    "postcss": "^8.2.6",
    "ramda": "^0.27.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hot-keys": "^2.6.2",
    "react-parallax-tilt": "^1.4.90",
    "react-terminal-component": "^1.5.0",
    "tailwindcss": "^2.0.3",
    "twin.macro": "^2.2.3",
    "typescript": "^4.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.1",
    "@svgr/webpack": "^5.5.0",
    "@types/jest": "^26.0.20",
    "@types/mdx-js__react": "^1.5.3",
    "@types/node": "^14.14.31",
    "@types/ramda": "^0.27.38",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-macros": "^3.0.1",
    "eslint": "^7.20.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.0.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-html": "^6.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-tailwind": "^0.2.1",
    "husky": "^5.1.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "patch-package": "^6.2.2",
    "pinst": "^2.1.6",
    "prettier": "^2.2.1",
    "styled-components": "^5.2.1"
  },
  "license": "MIT"
}
