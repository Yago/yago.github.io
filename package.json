{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Yann Gouffon <hello@yago.io>",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "cloudinary-react": "^1.1.0",
    "gatsby": "^2.0.118",
    "gatsby-image": "^2.0.29",
    "gatsby-plugin-catch-links": "^2.0.9",
    "gatsby-plugin-manifest": "^2.0.16",
    "gatsby-plugin-offline": "^2.0.23",
    "gatsby-plugin-react-helmet": "^3.0.5",
    "gatsby-plugin-sass": "^2.0.10",
    "gatsby-remark-copy-linked-files": "^2.0.8",
    "gatsby-remark-prismjs": "^3.2.3",
    "gatsby-source-filesystem": "^2.0.19",
    "gatsby-transformer-remark": "^2.2.2",
    "graphql-skip-limit": "^2.0.4",
    "prismjs": "^1.15.0",
    "prop-types": "^15.6.2",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-helmet": "^5.2.0",
    "react-redux": "^6.0.0",
    "react-transition-group": "^2.5.3",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.7",
    "redux-thunk": "^2.3.0",
    "rehype-react": "^3.1.0"
  },
  "devDependencies": {
    "eslint-config-airbnb": "^17.1.0",
    "node-sass": "^4.11.0"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
      "browser": true
    },
    "rules": {
      "arrow-body-style": 0,
      "camelcase": 0,
      "global-require": 0,
      "import/extensions": 0,
      "import/no-unresolved": 0,
      "react/forbid-prop-types": [
        "any"
      ]
    }
  }
}
