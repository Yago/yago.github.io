{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Yann Gouffon <hello@yago.io>",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "picup": "cd ../500px_scraper/ && npm run start && cd ../2019/ && cp ../500px_scraper/pictures.json ./src/config/pictures.json && cp ../500px_scraper/download/* ./content/images/portfolio/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.3.1",
    "cloudinary-react": "^1.1.0",
    "gatsby": "^2.7.1",
    "gatsby-image": "^2.1.1",
    "gatsby-plugin-catch-links": "^2.0.15",
    "gatsby-plugin-google-analytics": "^2.0.20",
    "gatsby-plugin-manifest": "^2.1.1",
    "gatsby-plugin-offline": "^2.1.1",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-sass": "^2.0.11",
    "gatsby-plugin-sharp": "^2.1.1",
    "gatsby-remark-copy-linked-files": "^2.0.12",
    "gatsby-remark-emoji": "^0.0.2",
    "gatsby-remark-prismjs": "^3.2.9",
    "gatsby-source-filesystem": "^2.0.37",
    "gatsby-transformer-remark": "^2.3.12",
    "gatsby-transformer-sharp": "^2.1.19",
    "graphql-skip-limit": "^2.0.6",
    "highcharts": "^7.1.1",
    "highcharts-react-official": "^2.1.3",
    "javascript-terminal": "^1.0.2",
    "moment": "^2.24.0",
    "photoswipe": "^4.1.3",
    "prismjs": "^1.16.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-calendar-heatmap": "^1.8.0",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-hot-keys": "^1.3.1",
    "react-photo-gallery": "^7.0.2",
    "react-redux": "^7.0.3",
    "react-reveal": "^1.2.2",
    "react-terminal-component": "^1.4.0",
    "react-tilt": "^0.1.4",
    "react-transition-group": "^4.0.1",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.7",
    "redux-thunk": "^2.3.0",
    "rehype-react": "^3.1.0"
  },
  "devDependencies": {
    "eslint-config-airbnb": "^17.1.0",
    "node-sass": "^4.12.0"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
      "browser": true
    },
    "rules": {
      "arrow-body-style": 0,
      "camelcase": 0,
      "global-require": 0,
      "import/extensions": 0,
      "import/no-unresolved": 0,
      "react/forbid-prop-types": [
        "any"
      ]
    }
  }
}
