    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "patch-package": "^6.4.7",
    "pinst": "^2.1.6",
    "prettier": "^2.2.1",
    "styled-components": "^5.2.3",
    "terser-webpack-plugin": "^5.1.1"
