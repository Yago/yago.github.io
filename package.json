    "react-hot-keys": "^2.4.5",
    "react-infinite-scroll-component": "^5.0.4",
    "react-loader-spinner": "^3.1.5",
    "react-photo-gallery": "^8.0.0",
    "react-redux": "^7.1.3",
    "react-reveal": "^1.2.2",
