{
  "name": "with-typescript-types",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf .next && rm -rf node_modules/.cache",
    "tailwind:build": "yarn run clean && tailwind build src/styles/tailwind.css -o src/styles/base.css -c ./tailwind.config.js",
    "dev": "yarn run tailwind:build && next",
    "build": "yarn run tailwind:build && yarn run config:update && next build",
    "start": "next start",
    "type-check": "tsc",
    "lint:js": "eslint --ext ts,tsx ./ --max-warnings=0",
    "postinstall": "husky install && patch-package",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "config:update": "./bin/updateSitemap.js && ./bin/updatePictureDb.js && ./bin/updateContribDb.js"
  },
  "dependencies": {
    "@emotion/babel-preset-css-prop": "^11.2.0",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
