    "@mdx-js/react": "^1.6.22",
    "@next/mdx": "^10.1.3",
    "@sergeymyssak/nextjs-sitemap": "^1.1.1",
    "@tailwindcss/aspect-ratio": "^0.2.0",
    "@tailwindcss/typography": "^0.4.0",
    "autoprefixer": "^10.2.5",
    "axios": "^0.21.1",
    "date-fns": "^2.19.0",
    "directory-tree": "^2.2.7",
    "framer-motion": "^4.1.2",
    "fs-extra": "^9.1.0",
    "image-size": "^0.9.7",
    "javascript-terminal": "^1.1.1",
    "next": "10.1.3",
    "next-pwa": "^5.1.3",
    "photoswipe": "^4.1.3",
    "postcss": "^8.2.9",
    "prism-themes": "^1.6.0",
    "prismjs": "^1.23.0",
    "ramda": "^0.27.1",
    "react": "^17.0.2",
    "react-calendar-heatmap": "^1.8.1",
    "react-dom": "^17.0.2",
    "react-hotkeys-hook": "^3.3.0",
    "react-intersection-observer": "^8.31.0",
    "react-minimal-pie-chart": "^8.2.0",
    "react-parallax-tilt": "^1.4.96",
    "react-terminal-component": "^1.5.0",
    "remark-emoji": "^2.1.0",
    "tailwindcss": "^2.0.4",
    "twin.macro": "^2.3.2",
    "typescript": "^4.2.3"
